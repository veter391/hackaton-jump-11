{
  "extends":"../../tsconfig.json",
  "compilerOptions": {
    "target": "es2020",
    "types": ["node", "react"],
    "strict": false,
    "outDir": "./dist",
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts",
    "index.ts"
  ]
}